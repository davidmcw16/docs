{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Arkade API",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "openapi": "https://api.arkade.ai/openapi-client.json",
  "api": {
    "baseUrl": "https://api.arkade.ai",
    "auth": {
      "method": "key",
      "name": "X-API-Key"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "essentials/overview",
              "essentials/rate-limits",
              "essentials/errors"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Status",
          "href": "https://status.arkade.ai",
          "icon": "signal-bars"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/arkade",
          "icon": "github"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@arkade.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://dashboard.arkade.ai"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/arkade",
      "github": "https://github.com/arkade",
      "linkedin": "https://linkedin.com/company/arkade"
    }
  }
}
